 temp);\n            return {\n                ...state,\n                taksList: temp.filter(item => item.id !== action.payload)\n            }\n        default:\n            return state\n    }\n}\n\nconst Context = React.createContext();\n\n\nconst Home = () => {\n    const [state, dispatch] = useReducer(reducer, initialState)\n\n    const [inputValue, setInputValue] = useState('')\n\n    useEffect(() => {\n        fetch(API)\n            .then(resp => resp.json())\n            .then(resp => dispatch({ type: 'FETCH_SUCCESS', payload: resp }))\n            .catch(er => console.log('ERROR CONSOLE', er))\n    }, [])\n\n    return (\n        < Context.Provider value={dispatch}>\n            <div className=\"w-screen mt-20 grid place-items-center\">\n                <div className=\"w-2/3 p-8 shadow-lg\">\n                    <h1 className=\"m-10\">TODO LIST</h1>\n                    <input\n                        type=\"text\"\n                        placeholder=\"read a book\"\n                        className=\"shadow p-3 mx-4\"\n                        value={inputValue}\n                        onChange={e => setInputValue(e.target.value)}\n                    />\n                    <button\n                        className=\"mb-10 px-6 py-2  shadow-lg bg-gray-50\"\n                        onClick={() => {\n                            dispatch({ type: 'ADD-TASK', payload: inputValue })\n                            setInputValue('')\n                        }}\n                    >Add Task</button>\n                    {/* {state.loading ? 'Loading' : JSON.stringify(state, null, 2)} */}\n                    {console.log('current state: ', state)}\n                    {state.loading ? 'Loading' : <TaskList items={state.taksList} />}\n                </div>\n            </div>\n        </Context.Provider>\n    );\n}\n\nexport default Home;\n\nconst TaskList = ({ items }) => {\n    return items.map(item => <Task key={item.id} context={Context} {...item} />)\n\n}\n\n",["56","57"],"/home/ashikshezan/gitHub/todo/front-end/src/index.js",[],"/home/ashikshezan/gitHub/todo/front-end/src/components/Footer.jsx",["58","59","60","61"],"/home/ashikshezan/gitHub/todo/front-end/src/pages/Login.jsx",["62"],"/home/ashikshezan/gitHub/todo/front-end/src/pages/SignUp.jsx",["63"],"/home/ashikshezan/gitHub/todo/front-end/src/customHooks.js",[],"/home/ashikshezan/gitHub/todo/front-end/src/axios.js",[],"/home/ashikshezan/gitHub/todo/front-end/src/components/Logout.jsx",["64"],{"ruleId":"65","severity":1,"message":"66","line":5,"column":8,"nodeType":"67","messageId":"68","endLine":5,"endColumn":12},{"ruleId":"65","severity":1,"message":"69","line":9,"column":8,"nodeType":"67","messageId":"68","endLine":9,"endColumn":21},{"ruleId":"65","severity":1,"message":"70","line":14,"column":10,"nodeType":"67","messageId":"68","endLine":14,"endColumn":15},{"ruleId":"65","severity":1,"message":"71","line":14,"column":17,"nodeType":"67","messageId":"68","endLine":14,"endColumn":25},{"ruleId":"72","replacedBy":"73"},{"ruleId":"74","replacedBy":"75"},{"ruleId":"65","severity":1,"message":"76","line":1,"column":29,"nodeType":"67","messageId":"68","endLine":1,"endColumn":39},{"ruleId":"72","replacedBy":"77"},{"ruleId":"74","replacedBy":"78"},{"ruleId":"79","severity":1,"message":"80","line":14,"column":25,"nodeType":"81","endLine":14,"endColumn":38},{"ruleId":"79","severity":1,"message":"80","line":19,"column":25,"nodeType":"81","endLine":19,"endColumn":38},{"ruleId":"79","severity":1,"message":"80","line":24,"column":25,"nodeType":"81","endLine":24,"endColumn":38},{"ruleId":"79","severity":1,"message":"80","line":29,"column":25,"nodeType":"81","endLine":29,"endColumn":38},{"ruleId":"82","severity":1,"message":"83","line":39,"column":8,"nodeType":"84","endLine":39,"endColumn":22,"suggestions":"85"},{"ruleId":"65","severity":1,"message":"86","line":1,"column":17,"nodeType":"67","messageId":"68","endLine":1,"endColumn":25},{"ruleId":"65","severity":1,"message":"